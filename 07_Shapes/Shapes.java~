public class Shapes {
    public static void main(String[] args){
	Shapes s = new Shapes();
	System.out.print(s.box(4,5));
    }

    public String box( int a, int b) {
	int height = a;
	int width = b;
	String s = "";
	while (height > 0) {
	    while (width > 0) {
		s = s + "*";
		width = width - 1;
	    }
	    s = s + "\n";
	    width = b;
	}
        return s;
    }
}
